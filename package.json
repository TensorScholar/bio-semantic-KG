{
  "name": "medical-aesthetics-extraction-engine",
  "version": "1.0.0",
  "description": "Elite Medical Aesthetics Data Extraction Engine with Advanced NLP and Knowledge Graph",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "deno run --allow-all --watch src/index.ts",
    "build": "deno compile --allow-all --output dist/medical-extraction src/index.ts",
    "test": "deno test --allow-all",
    "test:watch": "deno test --allow-all --watch",
    "test:coverage": "deno test --allow-all --coverage=coverage",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "start": "deno run --allow-all dist/medical-extraction",
    "migrate": "deno run --allow-all src/infrastructure/persistence/migrations/migrate.ts"
  },
  "keywords": [
    "medical-aesthetics",
    "data-extraction",
    "nlp",
    "knowledge-graph",
    "persian",
    "english",
    "neo4j",
    "elasticsearch"
  ],
  "author": "Elite Technical Consortium",
  "license": "MIT",
  "engines": {
    "deno": ">=1.40.0"
  },
  "imports": {
    "@std/": "https://deno.land/std@0.208.0/",
    "@oak/": "https://deno.land/x/oak@v12.6.1/",
    "@neo4j/": "https://deno.land/x/neo4j@5.15.0/",
    "@elastic/": "https://deno.land/x/elasticsearch@8.8.0/",
    "@redis/": "https://deno.land/x/redis@v0.29.0/",
    "@playwright/": "https://deno.land/x/playwright@1.40.0/",
    "@opentelemetry/": "https://deno.land/x/opentelemetry@v1.0.0/",
    "fp-ts": "https://esm.sh/fp-ts@2.16.5",
    "io-ts": "https://esm.sh/io-ts@2.2.20",
    "zod": "https://esm.sh/zod@3.22.4"
  }
}
